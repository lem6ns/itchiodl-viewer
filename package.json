{
  "name": "itchiodl-viewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "rollup -c -w",
    "build": "npm run build:svelte && npm run build:neu",
    "build:svelte": "rollup -c",
    "build:neu": "neu build --release",
    "start:neu": "neu run",
    "watch:tailwind": "postcss public/css/tailwind.css -o public/css/app.css -w"
  },
  "devDependencies": {
    "@neutralinojs/neu": "^9.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.10.4"
  },
  "dependencies": {
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  }
}
